{
  "name": "github-bot",
  "license": "MIT",
  "private": true,
  "author": "Matt Travi",
  "description": "my instance of greenkeeper-keeper",
  "repository": "travi/greenkeeper-keeper",
  "bugs": "https://github.com/travi/greenkeeper-keeper/issues",
  "homepage": "https://github.com/travi/greenkeeper-keeper#readme",
  "scripts": {
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "tests:integration": "run-s tests:integration:base -- --tags 'not @wip'",
    "tests:integration:base": "cucumber-js test/integration --require-module babel-register --format-options '{\"snippetInterface\": \"async-await\"}' --exit",
    "tests:integration:debug": "DEBUG=test run-s tests:integration",
    "tests:integration:wip": "run-s tests:integration:base -- --tags @wip",
    "test": "run-s lint:* tests:*",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "build": "babel src --source-maps --out-dir lib",
    "start": "node lib/index.js",
    "postinstall": "run-s build",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload"
  },
  "engines": {
    "node": "8.9.x",
    "npm": "5.6.x"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/any": "1.7.2",
    "babel-cli": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "commitlint-config-travi": "1.2.26",
    "cucumber": "4.0.0",
    "cz-conventional-changelog": "2.1.0",
    "globstar": "1.0.0",
    "greenkeeper-lockfile": "1.13.2",
    "http-status-codes": "1.3.0",
    "husky": "0.15.0-rc.5",
    "markdownlint-cli": "0.7.1",
    "npm-run-all": "4.1.2"
  },
  "dependencies": {
    "@travi/hapi-github-webhooks": "2.0.0",
    "dotenv-safe": "5.0.1",
    "glue": "5.0.0",
    "good": "8.1.0",
    "good-console": "7.1.0",
    "good-squeeze": "5.0.2",
    "hapi-graceful-shutdown-plugin": "2.0.0",
    "hapi-greenkeeper-keeper": "2.0.0-beta.2"
  }
}
